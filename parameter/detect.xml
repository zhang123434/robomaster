<?xml version="1.0"?>
<opencv_storage>
<!--
  GRAY_EDIT - 是否调整灰度图参数
  - 1 Enable
  - 0 Disable
-->
<GRAY_EDIT>0</GRAY_EDIT>
<!--
  COLOR_EDIT - 是否调整颜色参数
  - 1 Enable
  - 0 Disable
-->
<COLOR_EDIT>0</COLOR_EDIT>
<!--
  RED_ARMOR_GRAY_TH   - 红色灰度参数 
  RED_ARMOR_COLOR_TH  - BGR 红色参数
  BLUE_ARMOR_GRAY_TH  - 蓝色灰度参数,直接对灰度图做阈值化的参数
  BLUE_ARMOR_COLOR_TH - GBR 蓝色参数,通道相减后做阈值化的阈值；
-->
<RED_ARMOR_GRAY_TH>80</RED_ARMOR_GRAY_TH>
<RED_ARMOR_COLOR_TH>15</RED_ARMOR_COLOR_TH>
<BLUE_ARMOR_GRAY_TH>100</BLUE_ARMOR_GRAY_TH>
<BLUE_ARMOR_COLOR_TH>20</BLUE_ARMOR_COLOR_TH>
<!--
  LIGHT_EDTI - 是否调整灯条参数
  - 1 Enable
  - 0 Disable
-->
<LIGHT_EDTI>0</LIGHT_EDTI>
<!--
  LIGHT_DRAW - 是否绘制灯条
  - 1 Enable
  - 0 Disable
-->
<LIGHT_DRAW>0</LIGHT_DRAW>

<!--
  LIGHT_RATIO_W_H_MIN - 灯条高宽比最小值
  LIGHT_RATIO_W_H_MAX - 灯条高宽比最大值
-->
<LIGHT_RATIO_W_H_MIN>1</LIGHT_RATIO_W_H_MIN>
<LIGHT_RATIO_W_H_MAX>18</LIGHT_RATIO_W_H_MAX>

<!--
  LIGHT_ANGLE_MIN - 灯条角度最小值
  LIGHT_ANGLE_MAX - 灯条角度最大值
-->
<LIGHT_ANGLE_MIN>45</LIGHT_ANGLE_MIN>
<LIGHT_ANGLE_MAX>45</LIGHT_ANGLE_MAX>

<!--
  LIGHT_PERIMETER_MIN - 灯条周长最小值
  LIGHT_PERIMETER_MAX - 灯条周长最大值
-->
<LIGHT_PERIMETER_MIN>16</LIGHT_PERIMETER_MIN>
<LIGHT_PERIMETER_MAX>6000</LIGHT_PERIMETER_MAX>
<!--
  ARMOR_EDIT - 是否调整击打哨兵预测模型
  - 1 Enable
  - 0 Disable
-->
<ARMOR_FORECAST>1</ARMOR_FORECAST>
<!--
  ARMOR_EDIT - 是否调整装甲板参数
  - 1 Enable
  - 0 Disable
-->
<ARMOR_EDIT>1</ARMOR_EDIT>
<!--
  ARMOR_DRAW - 是否绘制装甲板
  - 1 Enable
  - 0 Disable
-->
<ARMOR_DRAW>1</ARMOR_DRAW>
<!--
  ARMOR_HEIGHT_RATIO_MIN - 装甲板左右灯条高度比最小值
  ARMOR_HEIGHT_RATIO_MAX - 装甲板左右灯条高度比最大值
-->
<ARMOR_HEIGHT_RATIO_MIN>5</ARMOR_HEIGHT_RATIO_MIN>
<ARMOR_HEIGHT_RATIO_MAX>22</ARMOR_HEIGHT_RATIO_MAX>
<!--
  ARMOR_WIDTH_RATIO_MIN - 装甲板左右灯条宽度比最小值
  ARMOR_WIDTH_RATIO_MAX - 装甲板左右灯条宽度比最大值
-->
<ARMOR_WIDTH_RATIO_MIN>3</ARMOR_WIDTH_RATIO_MIN>
<ARMOR_WIDTH_RATIO_MAX>33</ARMOR_WIDTH_RATIO_MAX>
<!--
  ARMOR_Y_DIFFERENT - 左右灯条y的差值不超过灯条平均高度的倍数*0.1
-->
<ARMOR_Y_DIFFERENT>7</ARMOR_Y_DIFFERENT>
<!--
  ARMOR_HEIGHT_DIFFERENT - 左右灯条高度差值不超过灯条平均高度的倍数*0.1
-->
<ARMOR_HEIGHT_DIFFERENT>7</ARMOR_HEIGHT_DIFFERENT>
<!--
  ARMOR_ANGLE_DIFFERENT - 左右灯条角度差
-->
<ARMOR_ANGLE_DIFFERENT>90</ARMOR_ANGLE_DIFFERENT>
<!--
  ARMOR_SMALL_ASPECT_MIN - 装甲板最小范围
  ARMOR_TYPE_TH          - 大小装甲板区分范围
  ARMOR_BIG_ASPECT_MAX   - 装甲板最大范围
-->
<ARMOR_SMALL_ASPECT_MIN>9</ARMOR_SMALL_ASPECT_MIN>
<ARMOR_TYPE_TH>29</ARMOR_TYPE_TH>
<ARMOR_BIG_ASPECT_MAX>42</ARMOR_BIG_ASPECT_MAX>

<!-- 反小陀螺参数-->
  <spin_timeout>2000</spin_timeout>
  <min_spin_count>5</min_spin_count>
  <min_spin_speed>3.5</min_spin_speed>
  <min_x_delta_ratio>0.30</min_x_delta_ratio>
  <max_x_delta_ratio>3.0</max_x_delta_ratio>
  <min_y_delta_ratio>0.0</min_y_delta_ratio>
  <max_y_delta_ratio>1.2</max_y_delta_ratio>
<!-- 反小陀螺参数-->
</opencv_storage>